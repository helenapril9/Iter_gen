    "nested_list = [\n",
    "\t['a', 'b', 'c'],\n",
    "\t['d', 'e', 'f'],\n",
    "\t[1, 2, None],\n",
    "]\n",
    "\n",
    "def flat_generator(nl):\n",
    "    for l in nl: \n",
    "        for i in l:\n",
    "            yield i\n",
    "\n",
    "for item in  flat_generator(nested_list):\n",
    "\tprint(item)\n",
